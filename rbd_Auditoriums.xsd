<?xml version="1.0" standalone="yes"?>
<xs:schema id="GIADBSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.rustest.ru/giadbset">
  <xs:element name="GIADBSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="rbd_Auditoriums" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="REGION" type="xs:int" />
              <xs:element name="AuditoriumID" msdata:DataType="System.Guid, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" type="xs:string" />
              <xs:element name="StationID" msdata:DataType="System.Guid, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" type="xs:string" />
              <xs:element name="AuditoriumCode">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="4" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AuditoriumName">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="255" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="RowsCount" type="xs:int" />
              <xs:element name="ColsCount" type="xs:int" />
              <xs:element name="OrganizerOrder" type="xs:int" />
              <xs:element name="DeleteType" type="xs:int" />
              <xs:element name="LimitPotencial" type="xs:int" />
              <xs:element name="Imported" type="xs:anyType" minOccurs="0" />
              <xs:element name="CreateDate">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="(\d{4})-(\d{2})-(\d{2})(.{1})(\d{2}):(\d{2}):(\d{2}).(\d{2,})"/>
                  </xs:restriction>
                </xs:simpleType>  
              </xs:element>
              <xs:element name="UpdateDate">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="(\d{4})-(\d{2})-(\d{2})(.{1})(\d{2}):(\d{2}):(\d{2}).(\d{2,})?"/>
                  </xs:restriction>
                </xs:simpleType>  
              </xs:element>
              <xs:element name="ImportCreateDate" nillable="true" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="((\d{4})-(\d{2})-(\d{2})(.{1})(\d{2}):(\d{2}):(\d{2}).(\d{2,}))|NULL"/>
                  </xs:restriction>
                </xs:simpleType>  
              </xs:element>
              <xs:element name="ImportUpdateDate" nillable="true" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="((\d{4})-(\d{2})-(\d{2})(.{1})(\d{2}):(\d{2}):(\d{2}).(\d{2,}))|NULL"/>
                  </xs:restriction>
                </xs:simpleType>  
              </xs:element>
              <xs:element name="ExamForm" type="xs:int" />
              <xs:element name="VideoControl" type="xs:boolean" />
              <xs:element name="IsLab" type="xs:boolean" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
